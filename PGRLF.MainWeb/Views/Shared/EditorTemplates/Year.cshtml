@using PGRLF.MainWeb
@using PGRLF.MainWeb.Helpers
@model string
@{
    const int startYear = 1950;
    @Html.DropDownListFor(m => m, Enumerable.Range(startYear, DateTime.Now.Year - startYear + 1)
        .Reverse()
        .Select(year => new SelectListItem()
    {
        Text = year.ToString(),
        Value = year.ToString(),
        Selected = year.ToString() == Model
    })
    .PrependEmptyItem())
}